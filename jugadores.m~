function [Cs,ds] = jugadores()
    inicio = 1987;
    fin = 2016;
    anios = fin - inicio +1;
    Cs = cell(anios,1);
    ds = cell(anios,1);
    for i = 1987:2016
        fileID = fopen(strcat('estadisticasJugadores/equipos',int2str(i),'.txt'),'r');
        sizeA = [4 Inf];
        C = fscanf(fileID,'%f %f %f %f', sizeA);
        C = C';
        Cs(i) = [C];
        
        fileIDb = fopen('winratesConcatenados.txt','r');
        sizeb = [1 Inf];
        d = fscanf(fileIDb,'%f', sizeb);
        d = d';
        ds(i) = {[ds ; d]};
        fclose('all');
    end
end