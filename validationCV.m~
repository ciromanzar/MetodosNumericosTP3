function MSerrores = validationCV(As,bs,K)
    [,anios] = size(As);
    equipostotal = 0;
    A = [];
    b = [];
    for i = 1:anios
        
        equipostotal = equipostotal + 
        A = [A ; As(i)];
        b = [b ; bs(i)];
    end
    b
    [m,n] = size(K);
    MSerrores = [];
    for i = 1:n
        TrainAi = [];
        Trainbi = [];
        TestAi = [];
        Testbi = [];
        for j = 1:m;
            if K(j,i) == 1
                TrainAi = [TrainAi ; A(j,:)];
                Trainbi = [Trainbi ; b(j)];
            else
                TestAi = [TestAi ; A(j,:)];
                Testbi = [Testbi ; b(j)];
            end
        end
        x = lsqlin(TrainAi,Trainbi);
        bRes = TestAi*x;
        err = mean((bRes-Testbi).^2);
        MSerrores = [MSerrores err];
    end
end